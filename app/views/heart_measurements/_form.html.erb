<%= form_for @heart_measurement do |f| %>
  <div class="field">
    <%= f.label :topic_id, class: "form-control" %><br>
    <%= f.text_field :topic_id, disabled: true, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :patient_id, class: "form-control" %><br>
    <%= f.text_field :patient_id, disabled: true, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :visit_id, class: "form-control" %><br>
    <%= f.text_field :visit_id, disabled: true, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :hosp_type, class: "form-control" %><br>
    <%= f.text_field :hosp_type, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :location, class: "form-control" %><br>
    <%= f.text_field :location, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :admission_date, class: "form-control" %><br>
    <%= f.date_field :admission_date, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :description, class: "form-control" %><br>
    <%= f.text_field :description, class: "form-control" %>
  </div>
