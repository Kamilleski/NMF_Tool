  <div class="field">
    <%= f.label :time_ago_scale, class: "form-control" %><br>
    <%= f.text_field :time_ago_scale, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :note, class: "form-control" %><br>
    <%= f.text_field :note, class: "form-control" %>
  </div>
  <div class="field">
    <%= f.label :note, class: "form-control" %><br>
    <%= f.file_field :attachment, class: "form-control" %>
  </div>
  <div class="actions">
    <%= f.submit, class: "form-control" %>
  </div>
<% end %>
