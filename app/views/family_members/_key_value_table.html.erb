<table class="table">
  <tr>
    <th>
      KEY
    </th>
    <th>
      VALUE
    </th>
  </tr>
  <% fm.future_patient_data_hash.each do |k, v| %>
  <tr>
    <td>
      <%= k.to_s.titleize %>
    </td>
    <td>
      <% if k == 'deceased' %>
        <%= v == '1' ? true : false %>
      <% else %>
        <%= print_if_present(v) %>
      <% end %>
    </td>
  </tr>
  <% end %>
</table>
