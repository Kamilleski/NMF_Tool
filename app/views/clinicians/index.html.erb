<div class="row">
  <div class="col-sm-12">
    <div class="card">
      <div class="card-header">
        <h1>All Clinicians</h1>
      </div>
      <table class="table table-striped table-responsive" cellspacing="0" width="100%">
        <tr>
          <th>ID</th>
          <th>Last Name</th>
          <th>First Name</th>
        </tr>
        <% @clinicians.each do |c| %>
          <tr>
            <td><%= link_to c.id, clinician_path(c) %></td>
            <td><%= c.last_name %></td>
            <td><%= c.first_name %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
