<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Recent Tests</h3>
  </div>
  <% if tests.length != 0 %>
    <table class="table table-striped table-hover">
      <tr>
        <th>Date</th>
        <th>Type</th>
        <th>Result</th>
        <th>Actions</th>
      </tr>
      <% tests.each do |t| %>
        <tr>
          <td><%= display_test_date(t) %></td>
          <td><%= find_trail(t.topic_id) %></td>
          <td><%= print_if_present(t.result) %></td>
          <td><%= render 'tests/link_buttons', t: t %></td>
        </tr>
      <% end %>
    </table>
  <% else %>
    <div class="panel-body">
      Patient has not reported any testing.
    </div>
  <% end %>
</div>
