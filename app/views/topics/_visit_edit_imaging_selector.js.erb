$('#visit-edit-imaging-selector').change(function renderHeartForm() {
  switch (this.value) {
    case 'echocardiogram':
      console.log("echo");
      $("div#heart-imagery-hook").html("<%= j render 'topics/echocardiogram', f: f, imaging_tech: 'echo' %>");
      break;
    case 'CT':
      console.log("CT");
      $("div#heart-imagery-hook").html("<%= j render 'topics/echocardiogram', f: f, imaging_tech: 'CT' %>");
      break;
    case 'MRI':
      console.log("mri");
      $("div#heart-imagery-hook").html("<%= j render 'topics/echocardiogram', f: f, imaging_tech: 'MRI' %>");
      break;
  };
});
