{
  "env": {
    "production": {
      "presets": [
        "es2016",
        [
          "es2015",
          { "modules": false }
        ],
        "react",
        "react-optimize",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          { "spec": true }
        ]
      ]
    },
    "development": {
      "presets": [
        "es2016",
        [
          "es2015", { "modules": false }
        ],
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          { "spec": true }
        ]
      ]
    },
    "cli": {
      "presets": [
        "es2016",
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        [
          "transform-class-properties",
          { "spec": true }
        ]
      ]
    }
  }
}
